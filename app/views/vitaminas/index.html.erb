<p id="notice"><%= notice %></p>

<h1>Vitaminas</h1>

<table>
  <thead>
    <tr>
      <th>Marca</th>
      <th>Tipo</th>
      <th>Precio</th>
      <th>Presentacion</th>
      <th>Stock</th>
      <th>Liquido</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vitaminas.each do |vitamina| %>
      <tr>
        <td><%= vitamina.marca %></td>
        <td><%= vitamina.tipo %></td>
        <td><%= vitamina.precio %></td>
        <td><%= vitamina.presentacion %></td>
        <td><%= vitamina.stock %></td>
        <td><%= vitamina.liquido %></td>
        <td><%= link_to 'Show', vitamina %></td>
        <td><%= link_to 'Edit', edit_vitamina_path(vitamina) %></td>
        <td><%= link_to 'Destroy', vitamina, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vitamina', new_vitamina_path %>
